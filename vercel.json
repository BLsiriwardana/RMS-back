{
    "builds": [{
        "src": "server/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9", "python_version": "3.12.2" }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "server/wsgi.py"
        }
    ]
}
